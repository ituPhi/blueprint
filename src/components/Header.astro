---
import { Image } from "astro:assets";
import Card from "../components/Card.astro";
import cover from "../images/frontCover_OrionStars_Panama_Construction.jpg";
---

<div class="h-full mx-auto">
  <Image
    id="coverImage"
    src={cover}
    alt="Orion Stars Panama"
    class="object-cover mx-auto h-[400px] sm:h-[600px]"
    width={"1828"}
    height={"700"}
    loading="eager"
  />

  <Card />
</div>
